<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <log4net>
    <appender name="LogRollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="C:\\Log\\SEFE\\ServceManagerUtils\\Web\\ServceManagerUtils.Web.log" />
      <staticLogFileName value="true" />
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <preserveLogFileNameExtension value="true" />
      <datePattern value=".yyyy-MM-dd" />
      <maximumFileSize value="10MB" />
      <MaxSizeRollBackups value="100" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%date] [%-5level %c:%M] [T:%t] - %m%n" />
      </layout>
    </appender>
    <root>
      <level value="INFO" />
      <appender-ref ref="LogRollingFileAppender" />
    </root>
  </log4net>
  <appSettings>
	<add key="Server" value="" />
    <add key="AccessGroup" value="" />
    <add key="AllowedCORSLocations" value="http://localhost:4173;http://localhost:5173" />
	<add key="OAuth/AuthUrl" value="https://login.microsoftonline.com" />
	<add key="OAuth/TenantId" value="" />
	<add key="OAuth/ClientAppId" value="" />
	<add key="CloudIncidentOperatorRole" value="" />
  </appSettings>
